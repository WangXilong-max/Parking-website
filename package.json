{
  "name": "melbourne-parking-map",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "install:backend": "cd backend && npm ci",
    "build:frontend": "vite build",
    "start:backend": "cd backend && node src/server.js",
    "start": "node start.js",
    "dev:full": "concurrently \"npm run dev\" \"cd backend && npm run dev\"",
    "test:deployment": "node test-deployment.js",
    "test:prod": "node test-deployment.js https://parking-website-production.up.railway.app"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "date-fns": "^2.30.0",
    "mapbox-gl": "^3.14.0",
    "vue": "^3.5.18",
    "vue-chartjs": "^5.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "globals": "^16.3.0",
    "prettier": "3.6.2",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0"
  }
}
